{
  "name": "my-ip-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker which shows your IP and some details about your ISP",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development wrangler dev",
    "deploy": "wrangler publish",
    "build": "esbuild --bundle --sourcemap --outdir=dist --format=esm --target=esnext ./src/index.ts --loader:.css=text --loader:.liquid=text --loader:.png=binary --loader:.svg=text",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'"
  },
  "author": "Rensatsu",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.11.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260101.0",
    "@miniflare/tre": "^3.0.0-next.14",
    "@types/country-list": "^2.1.4",
    "@types/ms": "^2.1.0",
    "@types/ua-parser-js": "^0.7.39",
    "country-list": "^2.4.1",
    "esbuild": "^0.27.2",
    "git-revision-webpack-plugin": "^5.0.0",
    "http-status-codes": "^2.3.0",
    "liquidjs": "^10.24.0",
    "ms": "^2.1.3",
    "prettier": "^3.7.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "ua-parser-js": "^2.0.7",
    "wrangler": "^4.54.0"
  }
}
